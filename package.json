{
  "name": "pugtube",
  "version": "0.4.2",
  "private": true,
  "engines": {
    "node": ">=18.13.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "browserslist": [
    "defaults",
    "not ie <= 11"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "dir": "git ls-tree -r --name-only HEAD | tree --fromfile",
    "postinstall": "prisma generate",
    "serve": "concurrently \"pnpm:dev\" \"pnpm:worker\"",
    "format": "eslint --fix ./src",
    "migrate": "npx prisma db push",
    "worker": "npx inngest-cli@latest dev",
    "test": "concurrently -m ${MAX_CONCURRENT_TESTS:-1} \"pnpm:unit\"",
    "unit": "jest",
    "seed": "npx prisma db seed",
    "tunnel": "ngrok http --hostname=tunnel.pugtube.dev 3000",
    "postbuild": "next-sitemap",
    "reset": "npx prisma migrate reset --force --skip-seed",
    "shell": "npx tsx",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@aws-crypto/sha256-browser": "^4.0.0",
