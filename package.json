{
  "name": "pugtube",
  "version": "0.2.3",
  "private": true,
  "engines": {
    "node": "~=16.19.1"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "lint": "next lint",
    "dir": "git ls-tree -r --name-only HEAD | tree --fromfile",
    "postinstall": "prisma generate",
    "serve": "concurrently \"npm:dev\" \"npm:background\"",
    "type-check": "tsc --noEmit",
    "format": "eslint --fix ./src",
    "migrate": "npx prisma db push",
    "background": "tsx src/worker.ts",
    "godmode": "start-server-and-test serve http://localhost:3000 \"cypress open --e2e\"",
    "e2e-headed": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "e2e-prod-headed": "start-server-and-test start http://localhost:3000 \"cypress open --e2e\"",
    "e2e": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "e2e-prod": "start-server-and-test start http://localhost:3000 \"cypress run --e2e\"",
    "component-headed": "cypress open --component",
    "component": "cypress run --component",
    "test": "concurrently -m ${MAX_CONCURRENT_TESTS:-1} \"npm:unit\" \"npm:component\" \"npm:e2e\"",
    "unit": "jest",
    "prepare": "husky install",
    "seed": "npx prisma db seed",
    "postbuild": "next-sitemap"
  },
