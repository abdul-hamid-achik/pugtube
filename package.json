{
  "name": "pugtube",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "NODE_OPTIONS='-r next-logger' next start",
    "type-check": "tsc --noEmit",
    "format": "eslint --fix ./src",
    "migrate": "pnpx prisma db push",
    "background": "pnpx inngest-cli dev",
    "e2e": "CYPRESS_baseUrl=http://localhost:3000 start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "CYPRESS_baseUrl=http://localhost:3000 start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "component": "cypress open --component",
    "component:headless": "cypress run --component"
  },
